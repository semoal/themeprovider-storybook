{
  "name": "themeprovider-storybook",
  "version": "1.1.2",
  "description": "Use your styled-components themes on your favourite storybook",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "watch:storybook": "start-storybook -p 6006 -c example/.storybook",
    "watch:tsc": "tsc -w",
    "dev": "concurrently --kill-others \"npm run watch:tsc\" \"npm run watch:storybook\"",
    "build": "rimraf dist/ && tsc",
    "wolk": "cp package.json LICENSE README.md dist",
    "test": "rimraf dist && jest"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.12"
  },
  "peerDependencies": {
    "@storybook/addons": "^5.x",
    "@storybook/react": "^5.x",
    "react": "^16.x",
    "styled-components": "^4.x"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@storybook/addons": "^5.0.3",
    "@storybook/react": "^5.0.3",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.16",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.8.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "styled-components": "^4.3.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semoal/themeprovider-storybook.git"
  },
  "keywords": [],
  "author": " <Sergio Moreno>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/semoal/themeprovider-storybook/issues"
  },
  "homepage": "https://github.com/semoal/themeprovider-storybook#readme"
}
